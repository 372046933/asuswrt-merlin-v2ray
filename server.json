{
    "log": {
        "access": "/var/log/v2ray/access.log",
        "error": "/var/log/v2ray/error.log",
        "loglevel": "warning"
    },
    "dns": {
        "servers": [
            "https+local://dns.google/dns-query"
        ]
    },
    "inbounds": [
        {
            "protocol": "vmess",
            "port": 22334,
            "settings": {
                "clients": [
                    {
                        "id": "NEWUUID",
                        "alterId": 4,
                        "security": "none" // set none only if use quic
                    }
                ]
            },
            "streamSettings": {
                "network": "quic",
                "quicSettings": {
                    "security": "chacha20-poly1305",
                    "key": "set_yours_password_here",
                    "header": {
                        "type": "wechat-video"
                    }
                }
            }
        },
        {
            "protocol": "shadowsocks",
            "port": 22335, // 监听端口
            "settings": {
                "method": "chacha20-ietf-poly1305",
                "password": "set_yours_password_here"
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "settings": {}
        }
    ]
}
